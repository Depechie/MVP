    <ItemGroup>
        <None Remove="Resources\mvp_logo.svg" />
        <None Remove="Resources\Fonts\SourceSansPro-Bold.ttf" />
        <None Remove="Resources\Fonts\SourceSansPro-SemiBold.ttf" />
        <None Remove="Resources\Fonts\SourceSansPro-Regular.ttf" />
        <None Remove="Resources\Fonts\MVPBuzz.ttf" />
        <None Remove="Resources\loading.json" />
        <None Remove="Resources\loading_light.json" />
        <None Remove="Services\Demo\Files\getprofile.json" />
        <None Remove="Services\Demo\Files\getprofileimage.json" />
        <None Remove="Services\Demo\Files\getvisibilities.json" />
        <None Remove="Services\Demo\Files\getcontributiontypes.json" />
        <None Remove="Services\Demo\Files\getcontributionareas.json" />
        <None Remove="Services\Demo\Files\getcontributions.json" />
        <None Remove="Resources\oss.json" />
    </ItemGroup>
    <ItemGroup>
        <EmbeddedResource Include="Resources\mvp_logo.svg">
            <LogicalName>mvp_logo.svg</LogicalName>
        </EmbeddedResource>
        <EmbeddedResource Include="Resources\Fonts\SourceSansPro-Bold.ttf" />
        <EmbeddedResource Include="Resources\Fonts\SourceSansPro-SemiBold.ttf" />
        <EmbeddedResource Include="Resources\Fonts\SourceSansPro-Regular.ttf" />
        <EmbeddedResource Include="Resources\Fonts\MVPBuzz.ttf" />
        <EmbeddedResource Include="Resources\loading.json">
          <LogicalName>MVP.loading.json</LogicalName>
        </EmbeddedResource>
        <EmbeddedResource Include="Resources\loading_light.json">
          <LogicalName>MVP.loading_light.json</LogicalName>
        </EmbeddedResource>
        <EmbeddedResource Include="Services\Demo\Files\getprofile.json">
          <LogicalName>getprofile.json</LogicalName>
        </EmbeddedResource>
        <EmbeddedResource Include="Services\Demo\Files\getprofileimage.json">
          <LogicalName>getprofileimage.json</LogicalName>
        </EmbeddedResource>
        <EmbeddedResource Include="Services\Demo\Files\getvisibilities.json">
          <LogicalName>getvisibilities.json</LogicalName>
        </EmbeddedResource>
        <EmbeddedResource Include="Services\Demo\Files\getcontributiontypes.json">
          <LogicalName>getcontributiontypes.json</LogicalName>
        </EmbeddedResource>
        <EmbeddedResource Include="Services\Demo\Files\getcontributionareas.json">
          <LogicalName>getcontributionareas.json</LogicalName>
        </EmbeddedResource>
        <EmbeddedResource Include="Services\Demo\Files\getcontributions.json">
          <LogicalName>getcontributions.json</LogicalName>
        </EmbeddedResource>
        <EmbeddedResource Include="Resources\oss.json">
          <LogicalName>oss.json</LogicalName>
        </EmbeddedResource>
    </ItemGroup>
    <ItemGroup>
        <Compile Update="Resources\Translations.Designer.cs">
            <DependentUpon>Translations.resx</DependentUpon>
        </Compile>
        <Compile Update="Resources\Translations.nl.Designer.cs">
            <DependentUpon>Translations.nl.resx</DependentUpon>
        </Compile>
    </ItemGroup>
    <ItemGroup>
        <EmbeddedResource Update="Resources\Translations.resx">
            <Generator>ResXFileCodeGenerator</Generator>
            <LastGenOutput>Translations.Designer.cs</LastGenOutput>
        </EmbeddedResource>
        <EmbeddedResource Update="Resources\Translations.nl.resx">
            <Generator>ResXFileCodeGenerator</Generator>
            <LastGenOutput>Translations.nl.Designer.cs</LastGenOutput>
        </EmbeddedResource>
    </ItemGroup>
