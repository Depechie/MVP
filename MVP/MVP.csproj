        <PackageReference Include="Xamarin.Essentials" Version="1.6.0" />
        <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
        <PackageReference Include="Microsoft.Identity.Client" Version="4.24.0" />
        <PackageReference Include="Xamarin.FFImageLoading.Forms" Version="2.4.11.982" />
        <PackageReference Include="Xamarin.FFImageLoading.Transformations" Version="2.4.11.982" />
        <PackageReference Include="PropertyChanged.Fody" Version="3.3.1" />
        <PackageReference Include="Xamarin.FFImageLoading.Svg.Forms" Version="2.4.11.982" />
        <PackageReference Include="Acr.UserDialogs" Version="7.1.0.470" />
        <PackageReference Include="Microsoft.AppCenter" Version="4.0.0" />
        <PackageReference Include="Microsoft.AppCenter.Analytics" Version="4.0.0" />
        <PackageReference Include="Microsoft.AppCenter.Crashes" Version="4.0.0" />
        <PackageReference Include="Refit" Version="5.2.1" />
        <PackageReference Include="HtmlAgilityPack" Version="1.11.29" />
        <PackageReference Include="akavache" Version="7.1.1" />
        <PackageReference Include="Xamarin.CommunityToolkit" Version="1.0.0-preview1119" />
        <PackageReference Include="Com.Airbnb.Xamarin.Forms.Lottie" Version="4.0.8" />
        <PackageReference Include="TinyMvvm.Autofac" Version="2.4.14" />
        <PackageReference Include="TinyMvvm.Forms" Version="2.4.14" />
    </ItemGroup>
    <ItemGroup>
        <Folder Include="Models\" />
        <Folder Include="Services\Helpers\" />
        <Folder Include="Styles\" />
        <Folder Include="Extensions\" />
        <Folder Include="Resources\" />
        <Folder Include="Services\Interfaces\" />
        <Folder Include="Resources\Fonts\" />
        <Folder Include="Converters\" />
        <Folder Include="Templates\" />
        <Folder Include="Helpers\" />
        <Folder Include="Pages\Base\" />
        <Folder Include="ViewModels\" />
        <Folder Include="ViewModels\Base\" />
        <Folder Include="Controls\" />
        <Folder Include="ViewModels\Data\" />
        <Folder Include="Validation\" />
        <Folder Include="Models\Enums\" />
        <Folder Include="Services\Demo\" />
        <Folder Include="Services\Demo\Files\" />
